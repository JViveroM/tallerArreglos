		baraja[0]="A corazon";	baraja[13]="A trebol"; 	baraja[26]="A Diamante";	baraja[39]="A picas"; 
		baraja[1]="2 corazon";	baraja[14]="2 trebol";	baraja[27]="2 Diamante";	baraja[40]="2 picas";
