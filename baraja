		baraja[3]="4 corazon";	baraja[16]="4 trebol";	baraja[29]="4 Diamante";	baraja[42]="4 picas";
		baraja[4]="5 corazon";	baraja[17]="5 trebol";	baraja[30]="5 Diamante";	baraja[43]="5 picas";
		baraja[5]="6 corazon";	baraja[18]="6 trebol";	baraja[31]="6 Diamante";	baraja[44]="6 picas";
		baraja[6]="7 corazon";	baraja[19]="7 trebol";	baraja[32]="7 Diamante";	baraja[45]="7 picas";
		baraja[7]="8 corazon";	baraja[20]="8 trebol";	baraja[33]="8 Diamante";	baraja[46]="8 picas";
		baraja[8]="9 corazon";	baraja[21]="9 trebol";	baraja[34]="9 Diamante";	baraja[47]="9 picas";
