		baraja[12]="K corazon";	baraja[25]="K trebol";	baraja[38]="K Diamante";	baraja[51]="K picas";
	}

	public String getBaraja(int b) {
		return baraja [b];
	}

	public void setBaraja(String[] baraja) {
		this.baraja = baraja;
	}
	
}
